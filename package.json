{
  "name": "bulk-ops-generator",
  "displayName": "Bulk Ops Generator",
  "description": "Generate bulk operations like cURL, SQL from CSV/TSV/JSON or normal list data.",
  "version": "0.2.0",
  "icon": "icon.png",
  "publisher": "Vuong",
  "repository": {
    "type": "git",
    "url": "https://github.com/vuon9/bulk-ops-generator"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "bulk-ops-generator.open",
        "title": "Bulk Ops Generator: Open"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm run check-types && npm run lint && node esbuild.js --watch",
    "package": "npm run check-types && npm run lint && node esbuild.js --minify",
    "check-types": "tsc --noEmit",
    "lint": "eslint .",
    "test": "node ./out/test/runTest.js",
    "vscode:package": "npm run package && vsce package",
    "vscode:publish": "npm run package && vsce publish"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.80.0",
    "esbuild": "^0.25.0",
    "eslint": "^8.57.1",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.50.1"
  }
}